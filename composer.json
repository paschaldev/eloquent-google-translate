{
    "name": "paschaldev/eloquent-translate",
    "description": "A library that automatically translates Laravel models with  Translate API easily.",
    "type": "library",
    "license": "MIT",
    "keywords": ["eloquent", "google", "translate", "i18n", "laravel", "translatable", "paschaldev"],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ezeugwu Paschal",
            "email": "ezeugwupaschal@yahoo.com"
        }
    ],
    "require-dev": {
        "laravel/laravel": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "PaschalDev\\EloquentTranslate\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "PaschalDev\\EloquentTranslate\\Providers\\TranslateServiceProvider"
            ],
            "aliases": {
                "EloquentTranslate": "PaschalDev\\EloquentTranslate\\Facades\\EloquentTranslate"
            }
        }
    },
    "require": {
        "viniciusgava/google-translate-api": "dev-master"
    }
}
